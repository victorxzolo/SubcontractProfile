@{ Layout = "~/Views/Shared/_Layout.cshtml"; ViewBag.Title = "Engineer Profile";
}
<div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12 layout-spacing">

    <div class="skills layout-spacing ">
        <div class="widget-content widget-content-area">
            <h3 class="">Engineer</h3>

            <div class="widget-content1">

                <div class="form-row mb-4">


                    <div class="form-group col-md-6">
                        <label for="ddlLocationName">Location Name</label>
                        <select id="ddlLocationName" class="form-control " required></select>

                    </div>
                    <div class="form-group col-md-6">
                        <label for="ddlTeamName">Team Name (TH)</label>
                        <select id="ddlTeamName" class="form-control " required></select>
                    </div>

                    <div class="form-group col-md-6">
                        <label for="inputStaffName">Staff Name</label>
                        <input type="text" class="form-control" id="inputStaffName" placeholder="Team Name (TH)">
                    </div>
