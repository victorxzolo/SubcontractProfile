<h4>Team</h4>
<hr>
<div class="row">
    <div class="col-md-12 col-sm-12 col-xl-12 col-lg-12">
        <div class="table-responsive">
            <table class="table table-bordered table-hover table-striped table-checkable table-highlight-head mb-4" id="tbtabteam">
                <thead width="100%">
                    <tr width="100%">
                        <th class="text-center" width="5%">View</th>
                        <th width="10%">Register Type</th>
                        <th width="20%">Location Name</th>
                        <th width="10%">Team Code</th>
                        <th width="15%">Team Name (TH)</th>
                        <th width="15%">Team Name (En)</th>
                        <th width="15%">Storage Location</th>
                        <th width="10%">Ship To</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                </tbody>
            </table>
        </div>
    </div>

</div>


<div id="datateamModal" class="modal fade bd-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myExtraLargeModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 id="hTitleEdit" class="modal-title">ข้อมูล Team</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

            <div class="modal-body">
                <form id="frNew" class="needs-validation" novalidate>
                    <div class="container">

                        <div class="row">
                            <div class="col-sm-2">
                                <span style="color:red">*</span> <label> Location Name : </label>
                            </div>
                            <div class="col-sm-4">
                                <select id="ddllocationName_Th" style="width:300px" class="form-control" required></select>
                                <div class="invalid-feedback">
                                    กรุณาเลือกข้อมูล Location Name.
                                </div>
                            </div>

                            <div class="col-sm-2">
                                <label>Location Code :</label>
                            </div>
                            <div class="col-sm-4">
                                <input type="text" id="locationCode" style="width:300px" class="form-control" disabled />

                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-sm-2">
                                <span style="color:red">*</span> <label>Team Code :</label>
                            </div>
                            <div class="col-sm-4">
                                <input type="text" id="TeamCode" style="width:300px" class="form-control" required>
                                <div class="invalid-feedback">
                                    กรุณากรอกข้อมูล Team Code.
                                </div>
                            </div>

                            <div class="col-sm-2">
                                <label>Vendor Code :</label>
                            </div>
                            <div class="col-sm-4">
                                <input type="text" id="VendorCode" style="width:300px" class="form-control">
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-sm-2">
                                <span style="color:red">*</span> <label>Team Name(TH) :</label>
                            </div>
                            <div class="col-sm-4">
                                <input type="text" id="TeamNameT" style="width:300px" class="form-control" required>
                                <div class="invalid-feedback">
                                    กรุณากรอกข้อมูล Team Name(TH)
                                </div>
                            </div>
                            <div class="col-sm-2">
                                <label>Team Name(EN) :</label>
                            </div>
                            <div class="col-sm-4">
                                <input type="text" id="TeamNameE" style="width:300px" class="form-control">
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-sm-2">
                                <label>Storage Location :</label>
                            </div>
                            <div class="col-sm-4">
                                <input type="text" id="Storage" style="width:300px" class="form-control">
                            </div>
                            <div class="col-sm-2">
                                <label>Ship To :</label>
                            </div>
                            <div class="col-sm-4">
                                <input type="text" id="Ship" style="width:300px" class="form-control">
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-sm-2">
                                <label>Out of service storage Location:</label>
                            </div>
                            <div class="col-sm-4">
                                <input type="text" id="OutStorage" style="width:300px" class="form-control">
                            </div>
                            <div class="col-sm-2">
                                <label>Warranty MA :</label>
                            </div>
                            <div class="col-sm-4">
