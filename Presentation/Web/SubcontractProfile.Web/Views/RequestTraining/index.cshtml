                        function calculateCost(cost,count) {

                            var totalcost = 0;
                            totalcost = cost * count;
                            return totalcost;
                        }
                        function vat7(cost)
                        {
                            const formatter = new Intl.NumberFormat('en-US', {
                                minimumFractionDigits: 2,
                                maximumFractionDigits: 2,
                            });

                            var incvat = 0;
                            incvat = cost * (7 / 100);
                            incvat = formatter.format(incvat);
                           // alert(incvat);
                            return incvat;
                        }


