@{
}
    <style>
        .form-control {
            display: block;
            width: 100%;
            height: 34px;
            padding: 6px 12px;
            font-size: 14px;
            line-height: 1.42857143;
            color: #555;
            background-color: #fff;
            background-image: none;
            border: 1px solid #ccc;
            border-radius: 0;
            -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075);
            box-shadow: inset 0 1px 1px rgba(0,0,0,.075);
            -webkit-transition: border-color ease-in-out .15s,-webkit-box-shadow ease-in-out .15s;
            -o-transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
            transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
        }

        .btn-default {
            color: #8DC63F; /*before color #74a333*/
            background-color: #DDE1E4;
            border-color: #ccc;
        }
    </style>

    <body>
        <h3>Edit Location </h3>
        <hr style="height:2px;border-width:0;color:gray;background-color:gray">
        <div class="container">
            <div class="row">
                <div class="col-sm-2">
                    <label>Company Name :</label>
                </div>
                <div class="col-sm-10">
                    <label>xxxxxxxxxxxxxxx</label>
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-sm-2">
                    <label>Register Type</label>
                </div>
                <div class="col-sm-10">
                    <input type="radio" id="1" name="type" value="new">
                    <label for="new">New Subcontract</label>&nbsp;&nbsp;
                    <input type="radio" id="2" name="type" value="deal">
                    <label for="deal">Dealer</label>
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-sm-2">
                    <label>Location Code :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="locationCode" style="width:300px" class="form-control">
                </div>
                <div class="col-sm-2">
                    <label>Location Name Alias :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="locationAlias" style="width:300px" class="form-control">
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-sm-2">
                    <label>Location Name(TH) :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="LocationNameT" style="width:300px" class="form-control">
                </div>
                <div class="col-sm-2">
                    <label>Location Name(EH) :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="LocationNameE" style="width:300px" class="form-control">
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-sm-2">
                    <label>Storage Location :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="Storage" style="width:300px" class="form-control">
                </div>
                <div class="col-sm-2">
                    <label>Ship To :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="Ship" style="width:300px" class="form-control">
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-sm-2">
                    <label>Out of Service Storage location :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="OutStorage" style="width:300px" class="form-control">
                </div>
                <div class="col-sm-2">
                    <label>Vat branch Number :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="Vat" style="width:300px" class="form-control">
                </div>
            </div>
            <br />

            <h4><label style="color:#8DC63F">Contract Phone</label></h4>
            <hr style="height:1px;border-width:0;color:gray;background-color:gray">
            <div class="row">
                <div class="col-sm-2">
                    <label>Phone No :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="PhoneNo" style="width:300px" class="form-control">
                </div>
                <div class="col-sm-2">
                    <label>Company main contract phone:</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="MainPhone" style="width:300px" class="form-control">
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-sm-2">
                    <label>Installations contract phone:</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="InstallationsPhone" style="width:300px" class="form-control">
                </div>
                <div class="col-sm-2">
                    <label>Maintenence contract phone:</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="MaintenencePhone" style="width:300px" class="form-control">
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-sm-2">
                    <label>Installations contract phone:</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="InstallationsPhone" style="width:300px" class="form-control">
                </div>
                <div class="col-sm-2">
                    <label>Maintenence contract phone:</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="MaintenencePhone" style="width:300px" class="form-control">
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-sm-2">
                    <label>Inventory contract phone:</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="InventoryPhone" style="width:300px" class="form-control">
                </div>
                <div class="col-sm-2">
                    <label>Payment contract phone:</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="PaymentPhone" style="width:300px" class="form-control">
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-sm-2">
                    <label>Etc contract phone:</label>
                </div>
                <div class="col-sm-10">
                    <input type="text" id="EtcPhone" style="width:300px" class="form-control">
                </div>
            </div>
            <br />

            <h4><label style="color:#8DC63F">Contract Email</label></h4>
            <hr style="height:1px;border-width:0;color:gray;background-color:gray">
            <div class="row">
                <div class="col-sm-2">
                    <label>Company group mail :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="CompanyMail" style="width:300px" class="form-control">
                </div>
                <div class="col-sm-2">
                    <label>Installation contract mail:</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="InstallationMail" style="width:300px" class="form-control">
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-sm-2">
                    <label>Maintanence contract mail :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="MaintanenceMail" style="width:300px" class="form-control">
                </div>
                <div class="col-sm-2">
                    <label>Inventory contract mail:</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="InventoryMail" style="width:300px" class="form-control">
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-sm-2">
                    <label>Payment contract mail :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="PaymentMail" style="width:300px" class="form-control">
                </div>
                <div class="col-sm-2">
                    <label>Etc contract mail:</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="EtcMail" style="width:300px" class="form-control">
                </div>
            </div>
            <br />

            <h4><label style="color:#8DC63F">Address</label></h4>
            <hr style="height:1px;border-width:0;color:gray;background-color:gray">
            <div class="row">
                <div class="col-sm-2">
                    <label>Location Address :</label>
                </div>
                <div class="col-sm-4">
                    <textarea name="LocationAddress" rows="2" style="width:300px;"></textarea>
                </div>
                <div class="col-sm-2">
                    <label>Post Address :</label>
                </div>
                <div class="col-sm-4">
                    <textarea name="PostAddress" rows="2" style="width:300px;"></textarea>
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-sm-2">
                    <label>Tax Address :</label>
                </div>
                <div class="col-sm-4">
                    <textarea name="TaxAddress" rows="2" style="width:300px;"></textarea>
                </div><br />
                <div class="col-sm-2">
                    <label>WT Address :</label>
                </div>
                <div class="col-sm-4">
                    <textarea name="WTAddress" rows="2" style="width:300px;"></textarea>
                </div>
            </div>
            <br />

            <h4><label style="color:#8DC63F">Bank</label></h4>
            <hr style="height:1px;border-width:0;color:gray;background-color:gray">
            <div class="row">
                <div class="col-sm-2">
                    <label>Bank code :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="BankCode" style="width:300px" class="form-control">
                </div>
                <div class="col-sm-2">
                    <label>Bank Name :</label>
                </div>
                <div class="col-sm-4">
                    <div class="row col-sm-12">
                        <input type="text" id="BankName" style="width:280px" class="form-control">
                        <input type="button" id="btnBank" width="50" />
                    </div>
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-sm-2">
                    <label>Bank Account No :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="BankAccountNo" style="width:300px" class="form-control">
                </div>
                <div class="col-sm-2">
                    <label>Bank Account Name :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="BankAccountName" style="width:300px" class="form-control">
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-sm-2">
                    <label>Bank Branch No :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="BankBranchNo" style="width:300px" class="form-control">
                </div>
                <div class="col-sm-2">
                    <label>Bank Branch Name :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="BankBranchName" style="width:300px" class="form-control">
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-sm-2">
                    <label>Bank Attach File :</label>
                </div>
                <div class="col-sm-10">
                    <div class="row col-sm-12">
                        <input type="text" id="BankAttach" style="width:220px" class="form-control">
                        <input type="button" id="btnFile" value="Browse" width="50" />
                    </div>
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-sm-2">
                    <label>Panalty contract phone :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="PanaltyPhone" style="width:300px" class="form-control">
                </div>
                <div class="col-sm-2">
                    <label>Panalty contract mail :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="PanaltyMail" style="width:300px" class="form-control">
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-sm-2">
                    <label>Contract name :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="ContractName" style="width:300px" class="form-control">
                </div>
                <div class="col-sm-2">
                    <label>Contract email :</label>
                </div>
                <div class="col-sm-4">
                    <input type="text" id="ContractMail" style="width:300px" class="form-control">
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-sm-12" style="text-align:center; ">
                    <input type="button" id="btnBack" value="Back" width="100" class="btn btn-default" />&nbsp;&nbsp;
                    <input type="button" id="btnClear" value="Clear" width="100" class="btn btn-default" />&nbsp;&nbsp;
                    <input type="button" id="btnSubmit" value="Submit" width="100" class="btn btn-default" />
                </div>
            </div>
        </div>
    </body>
