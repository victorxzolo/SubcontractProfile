                                </div>
                                <div class="col-sm-4">
                                    <input type="text" id="OutStorage" style="width:300px" class="form-control">
                                </div>
                                <div class="col-sm-2">
                                    <label>@Localizer["WarrantyMA"] :</label>
                                </div>
