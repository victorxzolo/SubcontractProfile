

@using Microsoft.AspNetCore.Mvc.Localization

@inject IViewLocalizer Localizer

@{ Layout = "~/Views/Shared/_Layout.cshtml"; ViewBag.Title = "Team Profile";
}

<div id="pageID" style="display:none;">TeamProfile</div>

<div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12 layout-spacing">

    <div class="skills layout-spacing ">
        <div class="widget-content widget-content-area">
            <h3 class="">@Localizer["Team"]</h3>

            <div class="widget-content1">

                <div class="form-row mb-4">


                    <div class="form-group col-md-6">
                        <label for="inputLocationCodeMain">@Localizer["LocationName"]</label>
                        <select id="inputLocationName" class="form-control" data-live-search="true" required></select>

                    </div>
                    <div class="form-group col-md-6">
                        <label for="inputTeamNameAilas">@Localizer["TeamCode"]</label>
                        <input type="text" class="form-control" id="inputTeamCode" placeholder="Team Code">
                    </div>

                    <div class="form-group col-md-6">
                        <label for="inputTeamNameTh">@Localizer["TeamNameThai"]</label>
                        <input type="text" class="form-control" id="inputTeamNameTh" placeholder="Team Name (TH)">
                    </div>
                    <div class="form-group col-md-6">
                        <label for="inputTeamNameEn">@Localizer["TeamNameEng"]</label>
                        <input type="text" class="form-control" id="inputTeamNameEn" placeholder="Team Name (EN)">
                    </div>

                    <div class="form-group col-md-6">
                        <label for="inputStorageLocation">@Localizer["StorageLocation"]</label>
                        <input type="text" class="form-control" id="inputStorageLocation" placeholder="Storage Location">
                    </div>
                    <div class="form-group col-md-6">
                        <label for="inputShipto">@Localizer["ShipTo"]</label>
                        <input type="text" class="form-control" maxlength="13" id="inputShipto" placeholder="Ship To">
                    </div>

                </div>

                <div style="text-align:center">

                    <button id="btnMainClearLoc" type="button" class="btn btn-dark mb-2">
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"
                             stroke-linecap="round" stroke-linejoin="round" class="feather feather-x-circle">
                            <circle cx="12" cy="12" r="10"></circle>
                            <line x1="15" y1="9" x2="9" y2="15"></line>
                            <line x1="9" y1="9" x2="15" y2="15"></line>
                        </svg> @Localizer["Reset"]
                    </button>
                    <button id="btnSearchLoc" type="button" class="btn btn-success mb-2">
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"
                             stroke-linecap="round" stroke-linejoin="round" class="feather feather-search">
                            <circle cx="11" cy="11" r="8"></circle>
                            <line x1="21" y1="21" x2="16.65" y2="16.65"></line>
                        </svg>  @Localizer["Search"]
                    </button>

                </div>


            </div>


            <br />
            <div class="widget-heading">
                <h3 class="">@Localizer["SearchResult"]</h3>

            </div>
            <div class="col-md-12 text-right mb-1">
                <button id="btnAdd" type="button" class="btn btn-success mb-2">
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"
                         stroke-linecap="round" stroke-linejoin="round" class="feather feather-plus-circle">
                        <circle cx="12" cy="12" r="10"></circle>
                        <line x1="12" y1="8" x2="12" y2="16"></line>
                        <line x1="8" y1="12" x2="16" y2="12"></line>
                    </svg> @Localizer["Add"]
                </button>

            </div>


            <div class="table-responsive">
                <table id="tbSearchResult" class="table table-bordered table-hover table-striped table-checkable table-highlight-head mb-4">
                    <thead>
                        <tr>
                            <th class="text-center">@Localizer["Edit"]</th>
                            <th>@Localizer["LocationName"] </th>
                            <th>@Localizer["TeamNameThai"] </th>
                            <th>@Localizer["TeamNameEng"] </th>
                            <th>@Localizer["StorageLocation"] </th>
                            <th>@Localizer["ShipTo"] </th>
                        </tr>
                    </thead>
                </table>
            </div>

        </div>
    </div>

</div>

